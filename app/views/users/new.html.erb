<h1>Sign Up</h1>

<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <%= form_with(model: @user, local: true) do |f| %>
            <%= f.label :first_name %>
            <%= f.text_field :first_name %>

            <%= f.submit 'Create my Account', class: "btn btn-primary" %>
        <% end %>
    </div>
</div>
