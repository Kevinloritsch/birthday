<div class="spacer"></div>

<h1 style="text-align: center;  color: black; font-family: "Copperplate";">All Users</h1>

<ul class="users" style="text-align: center; font-family: "Copperplate";">
    <% @users.each do |user| %>
        <li>
            <%= user.birthday %> - <%= user.first_name %> <%= user.last_name %>
        </li>
    <% end %>
</ul>